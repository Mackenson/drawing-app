<% @paints.each do |paint| %>
  <%- if paint.user_id === current_user.id -%>
    <div id="paint-box">
    <img src= <%= paint.photo.url %> />
    <button class="rb"><%= link_to "deleted", paint_path(paint.id), :method=> :delete %></button>
    <button class="rb"><%= link_to "edit", edit_paint_path(paint.id) %></button>
  </div>
  <%- end -%>
<% end %>
